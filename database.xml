<?xml version="1.0" encoding="UTF-8"?>
<database>

    <global>

        <table name="rabbitmqserver_jobs">
            <field type="bigint(20) NOT NULL PRIMARY KEY AUTO_INCREMENT">id</field>
            <field type="VARCHAR(255) NOT NULL">jobWorker</field>
            <field type="text NOT NULL">jobData</field>
            <field type="text NULL">resultData</field>
            <field type="TINYINT(1) NOT NULL">status</field>
            <field type="SMALLINT NOT NULL">priority</field>
            <field type="TINYINT(1) NOT NULL">deleteOnFinish</field>
            <field type="INTEGER UNSIGNED NOT NULL">createTime</field>
            <field type="INTEGER UNSIGNED NOT NULL">lastUpdateTime</field>
        </table>

    </global>

</database>
